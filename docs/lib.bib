  journal = {Frontiers in Integrative Neuroscience},
  doi = {10.3389/fnint.2012.00093},
  author = {Hameroff, Stuart MD},
  year = {2012},
  keywords = {causal agency,causality,Consciousness,entanglement,free will,gamma synchrony,Gap Junctions,integrate-and-fire neuron,Microtubules,Orch OR,orchestrated objective reduction,Penrose-Hamerof,Quantum Theory,temporal non-locality,Volition,_tablet},
  file = {/home/coelschl/Zotero/storage/SB2GEPAK/Hameroff_2012_How quantum brain biology can rescue conscious free will.pdf}
}

