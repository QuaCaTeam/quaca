  keywords = {quantum refrigerator,quantum thermodynamics,quantum transport,spin bath,strong coupling},
  pages = {186},
  file = {/home/coelschl/Zotero/storage/3245AWQI/Katz and Kosloff - 2016 - Quantum Thermodynamics in Strong Coupling Heat Tr.pdf;/home/coelschl/Zotero/storage/KPQQFJ4D/186.html}
}

@article{perarnau-llobet2018,
