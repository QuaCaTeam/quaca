@article{romero-isart2010,
  title = {Toward Quantum Superposition of Living Organisms},
  volume = {12},
  issn = {1367-2630},
  abstract = {The most striking feature of quantum mechanics is the existence of superposition states, where an object appears to be in different situations at the same time. The existence of such states has been previously tested with small objects, such as atoms, ions, electrons and photons (Zoller et al 2005 Eur. Phys. J. D 36 203\textendash{}28), and even with molecules (Arndt et al 1999 Nature 401 680\textendash{}2). More recently, it has been shown that it is possible to create superpositions of collections of photons (Del{\'e}glise et al 2008 Nature 455 510\textendash{}14), atoms (Hammerer et al 2008 arXiv:0807.3358) or Cooper pairs (Friedman et al 2000 Nature 406 43\textendash{}6). Very recent progress in optomechanical systems may soon allow us to create superpositions of even larger objects, such as micro-sized mirrors or cantilevers (Marshall et al 2003 Phys. Rev. Lett. 91 130401; Kippenberg and Vahala 2008 Science 321 1172\textendash{}6; Marquardt and Girvin 2009 Physics 2 40; Favero and Karrai 2009 Nature Photon. 3 201\textendash{}5), and thus to test quantum mechanical phenomena at larger scales. Here we propose a method to cool down and create quantum superpositions of the motion of sub-wavelength, arbitrarily shaped dielectric objects trapped inside a high-finesse cavity at a very low pressure. Our method is ideally suited for the smallest living organisms, such as viruses, which survive under low-vacuum pressures (Rothschild and Mancinelli 2001 Nature 406 1092\textendash{}101) and optically behave as dielectric objects (Ashkin and Dziedzic 1987 Science 235 1517\textendash{}20). This opens up the possibility of testing the quantum nature of living organisms by creating quantum superposition states in very much the same spirit as the original Schr{\"o}dinger's cat `gedanken' paradigm (Schr{\"o}dinger 1935 Naturwissenschaften 23 807\textendash{}12, 823\textendash{}8, 844\textendash{}9). We anticipate that our paper will be a starting point for experimentally addressing fundamental questions, such as the role of life and consciousness in quantum mechanics.},
  language = {en},
  number = {3},
  journal = {New Journal of Physics},
  doi = {10.1088/1367-2630/12/3/033015},
  author = {{Romero-Isart}, Oriol and Juan, Mathieu L. and Quidant, Romain and Cirac, J. Ignacio},
  month = mar,
  year = {2010},
  pages = {033015},
  file = {/home/coelschl/Zotero/storage/W6SXGKCN/Romero-Isart et al. - 2010 - Toward quantum superposition of living organisms.pdf}
}

@article{chang2010,
